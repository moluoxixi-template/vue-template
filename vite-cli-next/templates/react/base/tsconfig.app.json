{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "types": [
      "vite/client"
    ],
    "composite": true
  },
  "include": [
    "src/**/*",
    "env.d.ts"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}

