/**
 * 路由配置（文件系统路由模式）
 * 使用 vite-plugin-pages 自动生成路由
 */

import { createBrowserRouter } from 'react-router-dom'
import { routes } from 'virtual:pages'
import Layout from '@/layouts'

/**
 * 路由配置
 */
const router = createBrowserRouter([
  {
    path: '/',
    element: <Layout />,
    children: routes,
  },
])

export { router }

