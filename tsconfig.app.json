{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "lib": [
      "DOM",
      "ESNext"
    ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "types": [
      "vite/client"
    ],
    "composite": true
  },
  "vueCompilerOptions": {
    "plugins": [
      "@vue-macros/volar/define-models",
      "@vue-macros/volar/define-slots",
      "@vue-macros/volar/define-props",
      "@vue-macros/volar/define-emits"
    ]
  },
  "exclude": [
    "dist",
    "node_modules",
    "cypress",
    "vite.config.ts"
  ]
}
