{
  "name": "vue-template",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=20.18.0"
  },
  "packageManager": "pnpm@10.8.0",
  "scripts": {
    "dev": "vite",
    "CICD": "tsx ./scripts/CICD.mts",
    "build": "vite build --mode production & tsx ./scripts/build.mts",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint:eslint": "eslint . --fix",
    "prepare": "node .husky/install.mjs",
    "lint-staged": "lint-staged",
    "commit": "git add . &&  git-cz"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue,css,scss,less}": [
      "eslint --fix --no-warn-ignored"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cjs"
    }
  },
  "dependencies": {
    "@element-plus/icons-vue": "catalog:build",
    "@moluoxixi/ajax-package": "catalog:build",
    "@sentry/vite-plugin": "catalog:build",
    "@sentry/vue": "catalog:build",
    "axios": "catalog:build",
    "crypto-js": "catalog:build",
    "element-plus": "catalog:build",
    "lodash-es": "catalog:build",
    "pinia": "catalog:build",
    "pinia-plugin-persistedstate": "catalog:build",
    "radash": "catalog:build",
    "rxjs": "catalog:build",
    "uuid": "catalog:build",
    "vue": "catalog:build",
    "vue-i18n": "catalog:build",
    "vue-router": "catalog:build",
    "vxe-table": "catalog:build",
    "vite-plugin-qiankun": "catalog:build"
  },
  "devDependencies": {
    "@commitlint/cli": "catalog:dev",
    "@commitlint/config-conventional": "catalog:dev",
    "@moluoxixi/css-module-global-root-plugin": "catalog:dev",
    "@moluoxixi/eslint-config": "catalog:dev",
    "@moluoxixi/vite-config": "catalog:dev",
    "@moluoxixi/ajax-package": "catalog:dev",
    "@types/crypto-js": "catalog:type",
    "@types/lodash": "catalog:type",
    "@types/node": "catalog:type",
    "@types/uuid": "catalog:type",
    "@vue-macros/volar": "catalog:dev",
    "commitizen": "catalog:dev",
    "compressing": "catalog:dev",
    "cz-customizable": "catalog:dev",
    "dotenv": "catalog:dev",
    "@eslint-react/eslint-plugin": "catalog:dev",
    "eslint": "catalog:dev",
    "eslint-plugin-format": "catalog:dev",
    "husky": "catalog:dev",
    "jiti": "catalog:dev",
    "lint-staged": "catalog:dev",
    "npm-run-all2": "catalog:dev",
    "oxlint": "catalog:dev",
    "postcss": "catalog:dev",
    "sass": "catalog:dev",
    "sass-embedded": "catalog:dev",
    "sass-loader": "catalog:dev",
    "tailwindcss": "catalog:dev",
    "typescript": "catalog:dev",
    "vite": "catalog:dev",
    "vue-tsc": "catalog:dev"
  },
  "author": "1983531544@qq.com",
  "license": "MIT",
  "keywords": [
    "element-plus",
    "ElementPlus",
    "vue",
    "vite",
    "vue3",
    "ts",
    "eslint",
    "commitlint",
    "husky",
    "lint-staged",
    "typescript",
    "vite",
    "pinia",
    "router",
    "sass",
    "tailwindcss",
    "template"
  ]
}
